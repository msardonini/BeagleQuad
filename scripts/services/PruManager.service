[Unit]
Description=PruManager, owner and distriber of who gets access to use the PRU hardware
Requires=systemd-modules-load.service
After=network.target robotcontrol.service dbus-org.freedesktop.Avahi.service redis.service generic-board-startup.service redis.service

[Service]
Type=simple
PIDFile=/var/PruManager.pid
User=root
ExecStart=/home/debian/bin/PruManager

[Install]
WantedBy=multi-user.target
